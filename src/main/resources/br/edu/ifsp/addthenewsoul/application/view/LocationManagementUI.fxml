<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifsp.addthenewsoul.application.controller.LocationManagementUIController">
   <children>
      <Label layoutX="202.0" layoutY="14.0" text="LOCATION MANAGEMENT">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="40.0" layoutY="106.0" prefHeight="173.0" prefWidth="524.0">
        <columns>
          <TableColumn fx:id="cId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="cSection" prefWidth="75.0" text="SECTION" />
            <TableColumn fx:id="cNumber" prefWidth="108.0" text="NUMBER" />
            <TableColumn fx:id="cAssets" prefWidth="265.0" text="ASSETS" />
        </columns>
      </TableView>
      <TextField fx:id="txtId" layoutX="120.0" layoutY="73.0" prefHeight="25.0" prefWidth="94.0" />
      <Label layoutX="40.0" layoutY="77.0" text="Find location:" />
      <Button layoutX="225.0" layoutY="73.0" mnemonicParsing="false" onAction="#findLocationById" text="OK" />
      <Button layoutX="267.0" layoutY="73.0" mnemonicParsing="false" onAction="#findAllLocations" text="ALL LOCATIONS" />
      <Button layoutX="40.0" layoutY="290.0" mnemonicParsing="false" onAction="#addLocation" text="ADD NEW+" />
      <Button layoutX="129.0" layoutY="290.0" mnemonicParsing="false" onAction="#editLocation" prefHeight="25.0" prefWidth="67.0" text="EDIT" />
      <Button layoutX="202.0" layoutY="290.0" mnemonicParsing="false" onAction="#removeLocation" prefHeight="25.0" prefWidth="94.0" text="REMOVE" />
      <Button layoutX="301.0" layoutY="290.0" mnemonicParsing="false" onAction="#importCSV" prefHeight="25.0" prefWidth="130.0" text="IMPORT CSV" />
      <Button layoutX="434.0" layoutY="290.0" mnemonicParsing="false" onAction="#exportCSV" prefHeight="25.0" prefWidth="130.0" text="EXPORT CSV" />
      <Button layoutX="496.0" layoutY="328.0" mnemonicParsing="false" onAction="#backToPreviousScene" text="CANCEL" />
   </children>
</AnchorPane>
